{
    "name": "a17/laravel-cms-toolkit",
    "type": "library",
    "description": "A collection of Laravel helpers, traits and classes to quickly create custom content management apps for our clients",
    "keywords": [
        "A17",
        "cms-toolkit",
        "laravel",
        "cms"
    ],
    "homepage": "https://code.area17.com.com/a17/laravel-cms-toolkit",
    "license": "MIT",
    "authors": [
        {
            "name": "Quentin Renard",
            "email": "quentin@area17.com",
            "homepage": "https://area17.com/about/quentin-renard",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "laravel/framework": "~5.3.0|~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/contracts": "~5.3.0",
        "laracasts/flash": "1.3.4",
        "laravelcollective/html": "5.3.0",
        "dimsav/laravel-translatable": "6.0.1",
        "cartalyst/tags": "3.0",
        "league/flysystem-aws-s3-v3": "1.0.13",
        "myclabs/php-enum": "1.5.0",
        "imgix/imgix-php": "1.1.0",
        "mathiasgrimm/laravel-env-validator": "1.1.0",
        "lsrur/inspector": "dev-master",
        "barryvdh/laravel-debugbar": "^2.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/trevetts/inspector"
        }
    ],
    "require-dev": {
        "phpunit/phpunit" : "~5.0",
        "filp/whoops": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "A17\\CmsToolkit\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
